<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#189715"
    tools:context=".profilemanagement">


    <EditText
        android:id="@+id/editText7"
        android:layout_width="335dp"
        android:layout_height="56dp"
        android:background="@color/white"
        android:gravity="center"
        android:text="Username"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="159dp" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="335dp"
        android:layout_height="56dp"
        android:background="@color/white"
        android:layout_marginTop="25dp"
        android:gravity="center"
        android:text="Email"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/editText11"
        tools:layout_editor_absoluteX="40dp" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="335dp"
        android:layout_height="56dp"
        android:layout_marginTop="25dp"
        android:background="@color/white"
        android:gravity="center"
        android:text="Address"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/editText9"
        tools:layout_editor_absoluteX="42dp" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="335dp"
        android:layout_height="56dp"
        android:layout_marginTop="24dp"
        android:background="@color/white"
        android:gravity="center"
        android:text="Phone Number"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/editText8"
        tools:layout_editor_absoluteX="42dp" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="335dp"
        android:layout_height="56dp"
        android:layout_marginTop="25dp"
        android:background="@color/white"
        android:gravity="center"
        android:text="Position"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/editText7"
        tools:layout_editor_absoluteX="42dp" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="122dp"
        android:background="@drawable/logou"
        app:layout_constraintBottom_toTopOf="@+id/editText7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.288" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/black"
        android:backgroundTint="@color/white"
        android:layout_marginTop="25dp"
        android:text="Save"
        app:layout_constraintTop_toBottomOf="@+id/editText10"
        tools:layout_editor_absoluteX="161dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/botnavi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:foregroundGravity="bottom"
        android:background="#04CF00"
        app:itemTextColor="#fff"
        app:itemIconTint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/buttom_nav_menu">

    </com.google.android.material.bottomnavigation.BottomNavigationView>

</androidx.constraintlayout.widget.ConstraintLayout>